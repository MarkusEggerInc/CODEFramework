<UserControl x:Class="CODE.Framework.Services.Tools.Windows.ServiceTestHarnessUI" 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:t="clr-namespace:CODE.Framework.Services.Tools.Windows">
    <Grid>
        <Grid.Resources>
            <t:EditContractTemplateSelector x:Key="EditorTemplateSelector" />
        </Grid.Resources>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="3" />
            <ColumnDefinition Width="3*" />
        </Grid.ColumnDefinitions>

        <Image Name="Logo" Margin="5" Height="35" HorizontalAlignment="Left" />
        <StackPanel Grid.Column="3" Orientation="Horizontal" VerticalAlignment="Center" Margin="5" TextElement.FontSize="13.333">
            <Button Content="Invoke Operation" FontWeight="Bold" Click="InvokeService" ToolTip="Invoke the current operation (CTRL-I or CTRL-ENTER)" Padding="15,5" Margin="0,0,40,0" />
            <Button Margin="0,0,5,0" Content="Quick Save" ToolTip="Quick-Save the current message (CTRL-S)" Padding="5" Click="HandleQuickSave"/>
            <Button Margin="0,0,5,0" Content="Save As..." ToolTip="Save the current message" Padding="5" Click="HandleSaveAs"/>
            <Button Margin="5,0,5,0" Content="Quick Load" ToolTip="Quick-Load the last quick-saved message (CTRL-L or CTRL-O)" Padding="5" Click="HandleQuickLoad"/>
            <Button Margin="0,0,5,0" Content="Load..." ToolTip="Load an input message" Padding="5" Click="HandleLoad"/>
        </StackPanel>

        <Label FontFamily="Segoe UI Light" FontSize="22" Foreground="Black" Margin="8,0,0,0" Grid.Row="1" VerticalAlignment="Top" Content="Operations" />
        <ListBox x:Name="OperationsList" Grid.Row="2" Margin="8,0,0,8" SelectionChanged="NewOperationSelected">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel Orientation="Horizontal">
                        <Rectangle Fill="{DynamicResource MethodIcon}"  Height="16" Width="16" Margin="0,0,3,0" />
                        <TextBlock Text="{Binding Name}" Margin="3,1,0,1" />
                    </StackPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>

        <GridSplitter Grid.Column="1" Grid.Row="2" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

        <Label x:Name="OperationNameLabel" FontFamily="Segoe UI Light" FontSize="22" Foreground="Black" Grid.Row="1" Grid.Column="2" VerticalAlignment="Top" />
        <TabControl x:Name="ContractDisplayTab" Grid.Row="2" Grid.Column="2" Margin="0,0,8,8" TabStripPlacement="Bottom" SelectionChanged="HandleContractViewSwitched">
            <TabItem>
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Rectangle Height="16" Width="16" Margin="2" VerticalAlignment="Center" Fill="{DynamicResource ContractViewIcon}"/>
                        <Label Content="Contract" VerticalContentAlignment="Center" VerticalAlignment="Center"/>
                    </StackPanel>
                </TabItem.Header>
                <TreeView x:Name="RequestContractTree" ItemTemplateSelector="{StaticResource EditorTemplateSelector}" Grid.IsSharedSizeScope="True" BorderThickness="0">
                    <TreeView.ItemContainerStyle>
                        <Style TargetType="TreeViewItem">
                            <Setter Property="IsExpanded" Value="True" />
                            <Style.Triggers>
                                <Trigger Property="IsSelected" Value="True">
                                    <Setter Property="Foreground" Value="Black"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </TreeView.ItemContainerStyle>
                    <TreeView.Resources>
                        <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="Transparent"/>
                        <Style TargetType="TextElement">
                            <Setter Property="Foreground" Value="Black"/>
                        </Style>
                        <Style TargetType="TextBox" BasedOn="{StaticResource {x:Type TextBox}}">
                            <Setter Property="VerticalContentAlignment" Value="Center"/>
                            <Setter Property="MinWidth" Value="100"/>
                        </Style>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Root-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="25" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Fill="{DynamicResource MethodIcon}" Height="36" Width="36" Margin="0,3,3,3" Grid.Column="0" />
                                <StackPanel Grid.Column="1">
                                    <TextBlock Margin="0,8,0,0"><Run Text="{Binding MethodName}" FontSize="18" /><Run Text=" (" FontSize="14" /><Run Text="{Binding Name}" FontSize="14" FontWeight="Bold" /><Run Text=" is a " FontSize="14" /><Run Text="{Binding ContractType}" FontSize="14" FontWeight="Bold" /><Run Text=")" FontSize="14" /></TextBlock>
                                    <TextBox Text="{Binding OperationUrl}" Margin="0,4,0,10" IsReadOnly="True" BorderThickness="0" Background="{x:Null}" FontSize="14" />
                                </StackPanel>
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="SubObject-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Border Background="Red" Opacity=".2" Grid.ColumnSpan="6" Visibility="{Binding NullElementVisible}" />
                                <Rectangle Fill="{DynamicResource ObjectPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                                <Rectangle Fill="{DynamicResource WarningIcon}" Height="22" Width="22" Margin="2,1,4,1" Grid.Column="5" Visibility="{Binding NullElementVisible}" ToolTip="The value of this member is NULL. This element should be set to a proper default value in the contract." />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="List-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Border Background="Red" Opacity=".2" Grid.ColumnSpan="6" Visibility="{Binding NullElementVisible}" />
                                <Rectangle Fill="{DynamicResource CollectionPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <Button Content="+" Margin="0,1,0,1" ToolTip="Add item to list" Width="22" Grid.Column="3" HorizontalAlignment="Left"  Click="AddItemToList" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                                <Rectangle Fill="{DynamicResource WarningIcon}" Height="22" Width="22" Margin="2,1,4,1" Grid.Column="5" Visibility="{Binding NullElementVisible}" ToolTip="The value of this member is NULL. This element should be set to a proper default value in the contract." />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Generic-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Border Background="Red" Opacity=".2" Grid.ColumnSpan="6" Visibility="{Binding NullElementVisible}" />
                                <Rectangle Fill="{DynamicResource StringPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <TextBox Text="{Binding TextValue, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" MaxWidth="500" MinWidth="100" t:ControlFocusHelper.AutoMoveCursorToNextTreeItem="true" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                                <Rectangle Fill="{DynamicResource WarningIcon}" Height="22" Width="22" Margin="2,1,4,1" Grid.Column="5" Visibility="{Binding NullElementVisible}" ToolTip="The value of this member is NULL. This element should be set to a proper default value in the contract." />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Guid-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Border Background="Red" Opacity=".2" Grid.ColumnSpan="6" Visibility="{Binding NullElementVisible}" />
                                <Rectangle Fill="{DynamicResource GuidPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}">
                                    <TextBox Text="{Binding TextValue, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="230" />
                                    <Button Width="20" Margin="5,0,0,0" ToolTip="Insert Empty GUID" Click="EmptyGuid_Click">E</Button>
                                    <Button Width="20" Margin="5,0,5,0" ToolTip="Generate Random GUID" Click="RandomGuid_Click">R</Button>
                                </StackPanel>
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                                <Rectangle Fill="{DynamicResource WarningIcon}" Height="22" Width="22" Margin="2,1,4,1" Grid.Column="5" Visibility="{Binding NullElementVisible}" ToolTip="The value of this member is NULL. This element should be set to a proper default value in the contract." />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Boolean-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Fill="{DynamicResource BooleanPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <CheckBox IsChecked="{Binding BooleanValue, UpdateSourceTrigger=PropertyChanged}" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" VerticalAlignment="Center" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Integer-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Fill="{DynamicResource NumberPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <TextBox Text="{Binding TextValue, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Width="100" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" PreviewKeyDown="NumericNoDecimalKeyDown" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Long-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Fill="{DynamicResource NumberPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <TextBox Text="{Binding LongValue, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Width="100" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" PreviewKeyDown="NumericNoDecimalKeyDown" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Decimal-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Fill="{DynamicResource NumberPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <TextBox Text="{Binding TextValue, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Width="100" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" PreviewKeyDown="NumericWithDecimalKeyDown" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Float-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Fill="{DynamicResource NumberPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <TextBox Text="{Binding FloatValue, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Width="100" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" PreviewKeyDown="NumericWithDecimalKeyDown" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Double-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Fill="{DynamicResource NumberPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <TextBox Text="{Binding DoubleValue, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Width="100" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" PreviewKeyDown="NumericWithDecimalKeyDown" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Enum-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Fill="{DynamicResource EnumPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <ComboBox SelectedValue="{Binding EnumValue, UpdateSourceTrigger=PropertyChanged}" ItemsSource="{Binding EnumValues}" HorizontalAlignment="Left" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="DateTime-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Fill="{DynamicResource DateTimePropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <TextBox Text="{Binding DateTimeValue, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                            </Grid>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="ByteArray-Item">
                            <Grid MinHeight="23">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                                    <ColumnDefinition Width="10" />
                                    <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Rectangle Fill="{DynamicResource ByteArrayPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                                <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}">
                                    <TextBox Text="{Binding ByteArrayDisplayValue, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" IsReadOnly="True" HorizontalAlignment="Left" Margin="0,1,5,1" />
                                    <Button Width="25" Margin="5,0,0,0" ToolTip="Load from File" Click="LoadByteArrayFromFile_Click">...</Button>
                                </StackPanel>
                                <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                            </Grid>
                        </HierarchicalDataTemplate>
                    </TreeView.Resources>
                </TreeView>
            </TabItem>
            <TabItem>
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Rectangle Height="16" Width="16" Margin="2" VerticalAlignment="Center" Fill="{DynamicResource JsonViewIcon}"/>
                        <Label Content="JSON" VerticalContentAlignment="Center" VerticalAlignment="Center"/>
                    </StackPanel>
                </TabItem.Header>
                <TextBox FontFamily="Consolas" FontSize="12" x:Name="JsonContract" AcceptsReturn="True" AcceptsTab="True" TextWrapping="NoWrap" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" BorderThickness="0"/>
            </TabItem>
            <TabItem>
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Rectangle Height="16" Width="16" Margin="2" VerticalAlignment="Center" Fill="{DynamicResource XmlViewIcon}"/>
                        <Label Content="XML" VerticalContentAlignment="Center" VerticalAlignment="Center"/>
                    </StackPanel>
                </TabItem.Header>
                <TextBox FontFamily="Consolas" FontSize="12" x:Name="XmlContract" AcceptsReturn="True" AcceptsTab="True" TextWrapping="NoWrap" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" BorderThickness="0"/>
            </TabItem>
        </TabControl>
        <Label x:Name="FlashLabel" Content="Saved!" Foreground="#FF530A0A" Grid.Row="2" Grid.Column="2" Margin="0,0,8,8" VerticalAlignment="Center" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Visibility="Collapsed" RenderTransformOrigin="0.5,0.5">
        	<Label.RenderTransform>
        		<TransformGroup>
        			<ScaleTransform/>
        			<SkewTransform/>
        			<RotateTransform/>
        			<TranslateTransform/>
        		</TransformGroup>
        	</Label.RenderTransform>
        </Label>
    </Grid>

    <UserControl.Resources>
        <Pen x:Key="Pen1" Brush="#35000000" Thickness="4.520" StartLineCap="Round" EndLineCap="Round" DashCap="Round" LineJoin="Round" MiterLimit="4" ></Pen>
        <SolidColorBrush x:Key="IconBackground" Color="CornflowerBlue" />
        <SolidColorBrush x:Key="IconForeground" Color="White" />
        <DrawingBrush Stretch="Uniform" x:Key="DataContractImage">
            <DrawingBrush.Drawing>
                <DrawingGroup>
                    <DrawingGroup Transform="-0.700276017189026,0,0,0.250625014305115,123.61979675293,94.1557998657227">
                        <GeometryDrawing Geometry="M148.275,77.945A68.379,68.379,0.000,1,0,11.517,77.945A68.379,68.379,0.000,1,0,148.275,77.945z">
                            <GeometryDrawing.Brush>
                                <RadialGradientBrush Center="0.5,0.5" RadiusX="0.5" RadiusY="0.5" GradientOrigin="0.5,0.5" MappingMode="RelativeToBoundingBox">
                                    <GradientStop Color="#AD000000" Offset="0" ></GradientStop>
                                    <GradientStop Color="#59000000" Offset="0.417" ></GradientStop>
                                    <GradientStop Color="Transparent" Offset="1" ></GradientStop>
                                </RadialGradientBrush>
                            </GeometryDrawing.Brush>
                        </GeometryDrawing>
                    </DrawingGroup>
                    <DrawingGroup>
                        <DrawingGroup Transform="-0.700276017189026,0,0,0.250625014305115,150.613906860352,79.3344573974609">
                            <GeometryDrawing Geometry="M148.275,77.945A68.379,68.379,0.000,1,0,11.517,77.945A68.379,68.379,0.000,1,0,148.275,77.945z">
                                <GeometryDrawing.Brush>
                                    <RadialGradientBrush Center="0.5,0.5" RadiusX="0.5" RadiusY="0.5" GradientOrigin="0.5,0.5" MappingMode="RelativeToBoundingBox">
                                        <GradientStop Color="#AD000000" Offset="0" ></GradientStop>
                                        <GradientStop Color="#59000000" Offset="0.417" ></GradientStop>
                                        <GradientStop Color="Transparent" Offset="1" ></GradientStop>
                                    </RadialGradientBrush>
                                </GeometryDrawing.Brush>
                            </GeometryDrawing>
                        </DrawingGroup>
                        <DrawingGroup Transform="-0.235549002885818,0,0,0.235549002885818,145.546295166016,-37.0454483032227">
                            <GeometryDrawing Geometry="M397.320,453.502A172.748,172.748,0.000,1,0,51.824,453.502A172.748,172.748,0.000,1,0,397.320,453.502z">
                                <GeometryDrawing.Brush>
                                    <RadialGradientBrush Center="0.507,0.57" RadiusX="0.581" RadiusY="0.581" GradientOrigin="0.507,0.57" MappingMode="RelativeToBoundingBox">
                                        <GradientStop Color="#FFE3F9FF" Offset="0" ></GradientStop>
                                        <GradientStop Color="#FF73BEE7" Offset="0.362" ></GradientStop>
                                        <GradientStop Color="#FF438EC5" Offset="0.625" ></GradientStop>
                                        <GradientStop Color="#FF477DAF" Offset="0.726" ></GradientStop>
                                        <GradientStop Color="#FF020407" Offset="1" ></GradientStop>
                                    </RadialGradientBrush>
                                </GeometryDrawing.Brush>
                            </GeometryDrawing>
