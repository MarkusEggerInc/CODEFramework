                        <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                    </Grid>
                </HierarchicalDataTemplate>
                <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Decimal-Item">
                    <Grid MinHeight="23">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="{DynamicResource NumberPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                        <TextBox Text="{Binding TextValue}" HorizontalAlignment="Left" Width="100" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" PreviewKeyDown="NumericWithDecimalKeyDown" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                    </Grid>
                </HierarchicalDataTemplate>
                <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Float-Item">
                    <Grid MinHeight="23">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="{DynamicResource NumberPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                        <TextBox Text="{Binding FloatValue}" HorizontalAlignment="Left" Width="100" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" PreviewKeyDown="NumericWithDecimalKeyDown" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                    </Grid>
                </HierarchicalDataTemplate>
                <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Double-Item">
                    <Grid MinHeight="23">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="{DynamicResource NumberPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                        <TextBox Text="{Binding DoubleValue}" HorizontalAlignment="Left" Width="100" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" PreviewKeyDown="NumericWithDecimalKeyDown" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                    </Grid>
                </HierarchicalDataTemplate>
                <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Enum-Item">
                    <Grid MinHeight="23">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="{DynamicResource EnumPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                        <ComboBox SelectedValue="{Binding EnumValue}" ItemsSource="{Binding EnumValues}" HorizontalAlignment="Left" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                    </Grid>
                </HierarchicalDataTemplate>
                <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="DateTime-Item">
                    <Grid MinHeight="23">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="{DynamicResource DateTimePropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                        <TextBox Text="{Binding DateTimeValue}" HorizontalAlignment="Left" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                    </Grid>
                </HierarchicalDataTemplate>
            </TreeView.Resources>
        </TreeView>

        <Label x:Name="label2" FontFamily="Calibri" Grid.Row="2" Grid.ColumnSpan="3" />
            
      </Grid>
    
