<UserControl x:Class="CODE.Framework.Services.Tools.Windows.ServiceTestHarnessUI" 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ServiceTools="clr-namespace:CODE.Framework.Services.Tools.Windows">
    <Grid>
        <Grid.Resources>
            <ServiceTools:EditContractTemplateSelector x:Key="editorSelector" />
        </Grid.Resources>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="3" />
            <ColumnDefinition Width="3*" />
        </Grid.ColumnDefinitions>

        <Label FontFamily="Calibri" FontSize="16" Grid.Row="1" VerticalAlignment="Top" Content="Operations" />
        <ListBox x:Name="listView1" Grid.Row="1" Margin="8,30,0,8" SelectionChanged="NewOperationSelected">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel Orientation="Horizontal">
                        <Rectangle Fill="{DynamicResource MethodIcon}"  Height="16" Width="16" Margin="0,0,3,0" Grid.Column="0" />
                        <TextBlock Text="{Binding Name}" Margin="3,1,0,1" />
                    </StackPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
        
        <GridSplitter Grid.Column="1" Grid.Row="1" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />

        <Label x:Name="label1" FontFamily="Calibri" FontSize="16" Grid.Row="1" Grid.Column="2" VerticalAlignment="Top" />
        <TreeView x:Name="treeView1" Grid.Row="1" Grid.Column="2" Margin="0,30,8,8" ItemTemplateSelector="{StaticResource editorSelector}" Grid.IsSharedSizeScope="True">
            <TreeView.ItemContainerStyle>
                <Style>
                    <Setter Property="TreeViewItem.IsExpanded" Value="True" />
                </Style>
            </TreeView.ItemContainerStyle>
            <TreeView.Resources>
                <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Root-Item">
                    <Grid MinHeight="23">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="25" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="{DynamicResource DataContractImage}" Height="48" Width="48" Margin="0,0,3,0" Grid.Column="0" />
                        <StackPanel Grid.Column="1">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="{Binding MethodName}" FontSize="16" FontFamily="Calibri" />
                                <TextBlock Text=" (" FontSize="16" FontFamily="Calibri" />
                                <TextBlock Text="{Binding Name}" FontSize="16" FontFamily="Calibri" FontWeight="Bold" />
                                <TextBlock Text=")" FontSize="16" FontFamily="Calibri" />
                            </StackPanel>
                            <TextBlock Text="Input Parameter/Message sent to service operation" FontFamily="Calibri" />
                            <TextBlock Text="{Binding ContractType}" FontFamily="Calibri" Margin="0,0,0,10" />
                        </StackPanel>
                        <StackPanel Grid.Column="3" Orientation="Horizontal" VerticalAlignment="Top">
                            <Button Content="Invoke" FontWeight="Bold" Click="InvokeService" Padding="15,5" />
                            <Button Margin="20,0,0,0" Content="Get XML" Click="GetRestXmlClick" Padding="5" />
                            <Button Margin="5,0,0,0" Content="Get JSON" Click="GetRestJsonClick" Padding="5" />
                        </StackPanel>
                    </Grid>
                </HierarchicalDataTemplate>
                <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Generic-Item">
                    <Grid MinHeight="23">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Border Background="Red" Opacity=".2" Grid.ColumnSpan="6" Visibility="{Binding NullElementVisible}" />
                        <Rectangle Fill="{DynamicResource StringPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                        <TextBox Text="{Binding TextValue}" HorizontalAlignment="Left" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" MaxWidth="500" MinWidth="100" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                        <Rectangle Fill="{DynamicResource WarningIcon}" Height="22" Width="22" Margin="2,1,4,1" Grid.Column="5" Visibility="{Binding NullElementVisible}" ToolTip="The value of this member is NULL. This element should be set to a proper default value in the contract." />
                    </Grid>
                </HierarchicalDataTemplate>
                <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Guid-Item">
                    <Grid MinHeight="23">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Border Background="Red" Opacity=".2" Grid.ColumnSpan="6" Visibility="{Binding NullElementVisible}" />
                        <Rectangle Fill="{DynamicResource GuidPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}">
                            <TextBox Text="{Binding TextValue, Mode=TwoWay}" Width="230" />
                            <Button Width="20" Margin="5,0,0,0" ToolTip="Insert Empty GUID" Click="EmptyGuid_Click">E</Button>
                            <Button Width="20" Margin="5,0,5,0" ToolTip="Generate Random GUID" Click="RandomGuid_Click">R</Button>
                        </StackPanel>
                        <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                        <Rectangle Fill="{DynamicResource WarningIcon}" Height="22" Width="22" Margin="2,1,4,1" Grid.Column="5" Visibility="{Binding NullElementVisible}" ToolTip="The value of this member is NULL. This element should be set to a proper default value in the contract." />
                    </Grid>
                </HierarchicalDataTemplate>
                <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Boolean-Item">
                    <Grid MinHeight="23">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="{DynamicResource BooleanPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                        <CheckBox IsChecked="{Binding BooleanValue}" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" VerticalAlignment="Center" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                    </Grid>
                </HierarchicalDataTemplate>
                <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Integer-Item">
                    <Grid MinHeight="23">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="MemberName"  />
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="{DynamicResource NumberPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding Name}" Grid.Column="1" />
                        <TextBox Text="{Binding TextValue}" HorizontalAlignment="Left" Width="100" Margin="0,1,5,1" Grid.Column="3" Visibility="{Binding ManipulationTextBoxVisible}" PreviewKeyDown="NumericNoDecimalKeyDown" />
                        <TextBlock VerticalAlignment="Center" Text="{Binding MemberTypeFullText}" Grid.Column="4" Visibility="{Binding TypeVisible}" Margin="0,0,8,0" />
                    </Grid>
                </HierarchicalDataTemplate>
                <HierarchicalDataTemplate ItemsSource="{Binding SubItems}" x:Key="Long-Item">
                    <Grid MinHeight="23">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
