                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="Auto" SharedSizeGroup="ValueEntryControl" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Rectangle Fill="{DynamicResource NumberPropertyIcon}" Height="16" Width="16" Grid.Column="0" />
