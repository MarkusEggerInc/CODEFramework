<Window x:Class="CODE.Framework.Wpf.TestBench.ListDependencyProperties"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ListDependencyProperties" Height="600" Width="300">
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Button HorizontalAlignment="Center" Command="{Binding CopyProps}">Copy To Clipboard</Button>
        <ListBox ItemsSource="{Binding DependencyProps}" Grid.Row="1" />
    </Grid>
</Window>
